{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ES6",
    "target": "ES6",
    "downlevelIteration": true,
    "allowJs": true,
    "moduleResolution": "node16",
    "strict": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "allowImportingTsExtensions": true
  }
}
